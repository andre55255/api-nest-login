generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model user {
  id  String  @unique @default(uuid())
  email String  @unique
  hashPassword String
  createdAt DateTime  @default(now())
  updatedAt DateTime  @updatedAt()
  disabledAt DateTime?
}